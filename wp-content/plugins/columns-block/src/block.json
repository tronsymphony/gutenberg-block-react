{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/my-first-block",
	"version": "0.1.0",
	"title": "My First Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"attributes": {
		"downloadFileTitle":{
			"type": "string"
		},
		"downloadFileUrl": {
			"type": "string"
		},
		"downloadFileName": {
			"type": "string"
		},
		"mediaUrl": {
			"type": "string"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p"
		},
		"contentTitle": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"align": {
			"type": "string",
			"default": "none"
		},
		"backgroundColor": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"link": {
			"type": "string",
			"default": "https://kinsta.com"
		},
		"kaLink": {
			"type": "string",
			"default": ""
		},
		"linkLabel": {
			"type": "string",
			"default": "Check it out!"
		},
		"hasLinkNofollow": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "my-first-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}